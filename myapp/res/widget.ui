<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="MyWidget" parent="GtkWidget">
    <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
    <property name="halign">GTK_ALIGN_START</property>
    <property name="valign">GTK_ALIGN_START</property>
    <property name="name">widget name</property>
     
    <child>
      <object class="MyInfo" id="info"/>
    </child>
    <child>
      <object class="MyArea" id="area"/>
    </child>
    <child>
      <object class="MyOp" id="op"/>
    </child>
    <child>
      <object class="MyComment" id="comment"/>
    </child>
    <child>
      <object class="MyStats" id="stats"/>
    </child>
  </template>
</interface>
